<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:icon="@mipmap/icono_aplicacion"
        android:label="MyGymnastic"
        android:roundIcon="@mipmap/icono_aplicacion_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Splash">

        <activity
            android:name="com.example.mygymnastic2.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mygymnastic2.CrearEntrenamiento" />
        <activity android:name="com.example.mygymnastic2.SeleccionarEntrenamiento" />
        <activity android:name="com.example.mygymnastic2.RecomendacionesEntrenamiento"/>

    </application>

</manifest>